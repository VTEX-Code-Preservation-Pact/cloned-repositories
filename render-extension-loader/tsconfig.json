{
  "compilerOptions": {
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "allowJs": false,
    "sourceMap": true,
    "target": "es3",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots": ["node_modules/@types"]
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
    "./src/**/*",
    "./typings.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
