{
  "admin.app.reviews": {
    "around": ["layout"],
    "path": "/admin/app/reviews-ratings/*"
  },
  "admin.app.reviews.pending": {
    "component": "PendingReviewsTable",
    "path": "/admin/app/reviews-ratings/pending"
  },
  "admin.app.reviews.approved": {
    "component": "ApprovedReviewsTable",
    "path": "/admin/app/reviews-ratings/approved"
  },
  "admin.app.reviews.download": {
    "component": "DownloadReviewsTable",
    "path": "/admin/app/reviews-ratings/download"
  },
  "layout": {
    "component": "ReviewAdmin"
  }
}
